    "class Solution:\n",
    "    def hIndex(self, citations: list[int]) -> int:\n",
    "        \n",
    "        h = 0\n",
    "        citations.sort(reverse=True)\n",
    "\n",
    "        for n in citations:\n",
    "            if n <= h: break\n",
    "            else: h += 1\n",
    "        \n",
    "        return h\n",
    "\n",
    "sol = Solution()\n",
    "\n",
    "print(sol.hIndex([6,2,9,23]))"
   ]
  }
